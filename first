function Block(letter, rate, used, father,cod) {
this.letter = letter;
this.rate = rate;
this.used = used;
this.father = father;
this.code = code;
}

function FindCode(blok) {

if (tree[blok.father].code != '') {
 blok.code = tree[block.father].code + '1';
 }
